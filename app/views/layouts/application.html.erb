<!DOCTYPE html>
<html>
  <head>
    <title>Workspace</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= render '/layouts/nav' %>
  </head>

  <body>
    <div class="container">
      <%= yield %>
      <spam class="alert"><%= alert %></spam>
      <spam class="notice"><%= notice %></spam>
  </div>
  </body>
</html>
